<%- include('partials/header.ejs'); -%>

<h1>Home</h1>
<div class>
  <p><%= startingContent%></p>
  <a href="/compose">Compose new Post</a>
  <div>
    <% allPosts.forEach(function(post) {%>
    <div class="">
      <h2><%= post.title %></h2>
      <p>
        <%= post.content.substring(0,50) + " ..." %>
        <a href="posts/<%=post.title%>">Read More</a>
      </p>
      <%})%>

    </div>



  </div>

  <%- include('partials/footer.ejs'); -%>
